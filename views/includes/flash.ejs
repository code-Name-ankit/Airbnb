<% if(success && success.length){ %>
<div class="alert col-7  offset-3 mt-2 mb-2 alert-success " role="alert error-message" id="errorMsg">
<%= success %>

</div>
<% } %>


<% if(error && error.length){ %>
<div class="alert col-7  offset-3 mt-2 mb-2 alert-danger " role="alert error-message" id="errorMsg">
<%= error %>

</div>
<% } %>



